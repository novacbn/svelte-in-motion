{
    "name": "@svelte-in-motion/editor",
    "author": "novacbn",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev:application": "vite --host 0.0.0.0",
        "build:application": "vite build",
        "format": "npm run format:package",
        "format:package": "prettier --config ../../.prettierrc --plugin-search-dir=. --write ./src",
        "lint": "npm run lint:format",
        "lint:format": "prettier --config ../../.prettierrc --plugin-search-dir=. --check ./src",
        "preview:application": "vite preview --host 0.0.0.0 --port 3000",
        "test": "npm run test:types && npm run test:svelte",
        "test:svelte": "svelte-check --tsconfig ./tsconfig.json",
        "test:types": "tsc --noEmit"
    },
    "devDependencies": {
        "@deepkit/type-compiler": "^1.0.1-alpha.71",
        "@sveltejs/vite-plugin-svelte": "1.0.0-next.49",
        "@tsconfig/svelte": "^2.0.1",
        "@types/flexsearch": "^0.7.3",
        "@types/prismjs": "^1.26.0",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.7",
        "svelte-preprocess": "^4.9.8",
        "svelte2tsx": "^0.5.10",
        "tslib": "^2.3.1",
        "typescript": "^4.7.3",
        "vite": "2.9.12"
    },
    "dependencies": {
        "@codemirror/commands": "^6.0.1",
        "@codemirror/language": "^6.2.0",
        "@codemirror/search": "^6.0.0",
        "@codemirror/state": "^6.1.0",
        "@codemirror/theme-one-dark": "^6.0.0",
        "@codemirror/view": "^6.0.3",
        "@fluent/bundle": "^0.17.1",
        "@fluent/langneg": "^0.6.2",
        "@kahi-ui/framework": "0.6.4",
        "@novacbn/svelte-router": "0.0.2",
        "@svelte-in-motion/agent": "workspace:^",
        "@svelte-in-motion/animations": "workspace:^",
        "@svelte-in-motion/builtin-extensions": "workspace:^",
        "@svelte-in-motion/bundling": "workspace:^",
        "@svelte-in-motion/configuration": "workspace:^",
        "@svelte-in-motion/core": "workspace:^",
        "@svelte-in-motion/encoding": "workspace:^",
        "@svelte-in-motion/extension": "workspace:^",
        "@svelte-in-motion/rendering": "workspace:^",
        "@svelte-in-motion/screenshot": "workspace:^",
        "@svelte-in-motion/storage": "workspace:^",
        "@svelte-in-motion/temporal": "workspace:^",
        "@svelte-in-motion/type": "workspace:^",
        "@svelte-in-motion/utilities": "workspace:^",
        "buffer": "^6.0.3",
        "flexsearch": "^0.7.21",
        "lucide-svelte": "^0.61.0"
    }
}
